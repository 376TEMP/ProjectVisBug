{
  "name": "PixelBug",
  "version": "0.1.9",
  "description": "Open source browser design tools",
  "manifest_version": 2,
  "icons": { "128": "icons/pixelbug.png" },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "pixelbug.js"
    ]
  },
  "browser_action": {
    "default_title": "PixelBug"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+D",
        "mac": "Alt+Shift+D",
        "chromeos": "Alt+Shift+D",
        "linux": "Alt+Shift+D"
      }
    }
  }
}