{
  "name": "PixelBug",
  "version": "0.1.51",
  "description": "Hacking for Designers",
  "manifest_version": 2,
  "icons": { "128": "icons/128.png" },
  "devtools_page": "panel/index.html",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "pixelbug.js"
    ]
  },
  "browser_action": {
    "default_title": "PixelBug",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+D",
        "mac": "Alt+Shift+D",
        "chromeos": "Alt+Shift+D",
        "linux": "Alt+Shift+D"
      }
    }
  }
}